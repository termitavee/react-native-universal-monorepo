{
  "name": "react-native-universal-project",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "packages/app",
    "packages/browser-ext",
    "packages/electron",
    "packages/macos",
    "packages/windows",
    "packages/mobile",
    "packages/web",
    "packages/next",
    "packages/tv"
  ],
  "scripts": {
    "reset": "find . -type dir -name node_modules | xargs rm -rf && rm -rf yarn.lock",
    "android:metro": "npm run start -w mobile",
    "android:start": "npm run android -w mobile",
    "android:studio": "npm run studio -w mobile",
    "ios:metro": "npm run start -w mobile",
    "ios:start": "npm run ios -w mobile",
    "ios:xcode": "npm run xcode -w mobile",
    "ios:pods": "npm run pods -w mobile",
    "tv:android:metro": "npm start -w tv",
    "tv:android:start": "npm android -w tv",
    "tv:android:studio": "npm studio -w tv",
    "tv:tvos:metro": "npm start -w tv",
    "tv:tvos:start": "npm ios -w tv",
    "tv:tvos:xcode": "npm xcode -w tv",
    "tv:tvos:pods": "npm pods -w tv",
    "macos:metro": "npm start -w macos",
    "macos:start": "npm macos -w macos",
    "macos:xcode": "npm xcode -w macos",
    "macos:pods": "npm pods -w macos",
    "web:start": "npm start -w web",
    "web:build": "npm build -w web",
    "next:start": "npm dev -w next",
    "next:build": "npm build -w next",
    "next:serve": "npm start -w next",
    "electron:start": "npm start -w electron",
    "electron:package:mac": "npm package:mac -w electron",
    "electron:package:win": "npm package:win -w electron",
    "electron:package:linux": "npm package:linux -w electron",
    "browser-ext:start": "npm -ext start -w browser",
    "browser-ext:build": "npm -ext build -w browser",
    "windows:metro": "npm start -w windows",
    "windows:start": "npm windows -w windows",
    "lint": "npm run lint --workspaces --if-present",
    "lint:fix": "npm run lint:fix --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "typecheck": "ultra -r typecheck"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.71.12",
    "react-dom": "17.0.2",
    "react-native-web": "^0.17.1",
    "@react-native-async-storage/async-storage": "^1.15.7",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@craco/craco": "^6.2.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.9",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react-dom": "^17.0.9",
    "@types/react-native": "^0.64.13",
    "@types/react-test-renderer": "^18.0.0",
    "@types/react": "^18.0.24",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-jest": "^29.2.1",
    "babel-plugin-react-native-web": "^0.17.1",
    "babel-plugin-transform-define": "^2.0.1",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "electron-builder": "^22.11.7",
    "electron": "14.0.0",
    "electronmon": "^2.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react-app": "^6.2.2",
    "eslint-plugin-react-native": "^3.11.0",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-config": "^0.66.2",
    "metro-react-native-babel-preset": "0.73.10",
    "pod-install": "0.1.27",
    "prettier": "^2.4.1",
    "react-native-codegen": "^0.0.7",
    "react-native-monorepo-tools": "^1.1.0",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4",
    "ultra-runner": "^3.10.5",
    "wait-on": "^6.0.0"
  }
}
