{
  "extends": "../../tsconfig.json",
  "include": ["./src/*"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": ".",
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "isolatedModules": true,
    "jsx": "react-jsx"
  }
}
